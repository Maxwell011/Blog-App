<div id="posts_container">
  <div id="user_container">
  <img id="user_img" src="<%= @user.photo %>" alt="user profile">
      <div id="container_container_div">
        <h3><%= @user.name %></h3>
        <p>Number of posts: <%= @user.posts_counter %></p>
      </div>
    </div>
    <% @posts.each do |post|%>
    <div id="user_posts">
      <h3>Post #<%= post.id %></h3>
      <p><%= post.text %></p>
      <h4>Comments <%= post.comments_counter %> likes: <%= post.likes_counter %></h4>
       <%= form_with url: "/users/#{@user.id}/posts/#{post.id}/likes" do |f| %>
        <%= f.submit "like posts"%>
      <% end %>
    </div>
    <% post.last_five_comments.each do |comment| %>
        <div id="comments">
         <%= @user.name%>: <%= comment.text%>
        </div>
    <% end %>
  <% end %>
  <div id="button_container">
    <button id="pagination_button">Pagination</button>
  </div>
</div>